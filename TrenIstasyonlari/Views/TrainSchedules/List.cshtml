    <script>
        function Edit(id) {
            $.ajax({
                url: '@Url.Action("Edit")',
                type: 'POST',
                dataType: 'json',
                cache: false,
                async: true,
                data: new FormData(document.getElementById("frmTrainSchedulesEdit_" + id)),
                processData: false,
                contentType: false,
                success: function (data) {
                    alert(data)
                    if (data !== null) {
                        toastr.success("Istasyon Güncelleme Başarıyla Gerçekleştirildi", "Başarılı");
                        setTimeout(function () {
                            location.reload(true)
                        },
                            1500);

                    } else {
                        Swal.fire({
                            position: 'center',
                            icon: 'error',
                            title: "Güncelleme Başarısı",
                            showConfirmButton: true,
                            text: "Hata",
                            customClass: {
                                confirmButton: 'btn btn-primary'
                            },
                            buttonsStyling: true
                        });
                    }
                },
                error: function (data) {
                    if (data.result === null) {
                        Swal.fire({
                            position: 'center',
                            icon: 'error',
                            title: "Hata",
                            showConfirmButton: false,
                            timer: 1500,
                            customClass: {
                                confirmButton: 'btn btn-primary'
                            },
                            buttonsStyling: false
                        });
                    }


                }
            });
        }
        function Delete(id) {
            Swal.fire({
                title: "Emin misiniz",
                text: "Bu Seferi Silmeyi Onaylıyormusunuz?",
                icon: "warning",
                showCancelButton: true,
                confirmButtonText: "Onayla!",
                cancelButtonText: "Vazgeç!",
                reverseButtons: true
            }).then(function (result) {
                if (result.value) {
                    $.ajax({
                        url: '@Url.Action("Delete")/' + id,
                        type: 'GET',
                        success: function (data) {
                            alert(data)
                            if (data) {
                                toastr.success("Silme İşlemi Başarıyla Gerçekleştirildi", "Başarılı");
                                setTimeout(function () {
                                    location.reload(true)
                                },
                                    1500);

                            } else {

                                Swal.fire({
                                    position: 'center',
                                    icon: 'error',
                                    title: "Silme İşlemi Başarısız",
                                    showConfirmButton: true,
                                    text: data.Message,
                                    customClass: {
                                        confirmButton: 'btn btn-primary'
                                    },
                                    buttonsStyling: true
                                });
                            }
                        },
                        error: function (data) {
                            alert(data)
                            if (data.result === false) {
                                Swal.fire({
                                    position: 'center',
                                    icon: 'error',
                                    title: "Silme İşlemi Başarısız",
                                    showConfirmButton: false,
                                    timer: 1500,
                                    customClass: {
                                        confirmButton: 'btn btn-primary'
                                    },
                                    buttonsStyling: false
                                });
                            }


                        }
                    });
                } else if (result.dismiss === "cancel") {
                    Swal.fire(
                        "Vazgeçildi",
                        "Sefer Silmeden Vazgeçildi",
                        "error"
                    )
                }
            });
        }

    </script>
